@model IEnumerable<UniversityApplication.Models.Enrollment>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Semester)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Labgrade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Examgrade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Finalgrade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Feestatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Course)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Student)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Semester)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Labgrade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Examgrade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Finalgrade)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Feestatus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Course.Courseid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Student.Studentid)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Enrollmentid">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Enrollmentid">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Enrollmentid">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
